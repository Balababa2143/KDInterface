{
    "compilerOptions": {
        "strict": false,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "composite": true,
        "skipLibCheck": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
    },
    "include": [
        "@Type/**/*",
        "Script/**/*"
    ],
}