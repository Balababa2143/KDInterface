{
    "compilerOptions": {
        "strict": false,
        "outDir": "../Deploy/Script",
        "declarationDir": "../Deploy/@Type",
        "declaration": true,
        "sourceMap": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "composite": true,
        "skipLibCheck": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "noEmitOnError": false
    },
    "include": [
        "./**/*.ts"
    ],
    "references": [
        {
            "path": "../KinkyDungeon"
        }
    ]
}