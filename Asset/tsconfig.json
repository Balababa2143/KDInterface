{
    "compilerOptions": {
        "strict": true,
        "alwaysStrict": true,
        "strictBindCallApply": true,
        "outDir": "../Deploy",
        "declarationDir": "../Deploy",
        "declaration": true,
        "sourceMap": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "checkJs": true,
        "composite": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "rootDir": "./Src"
    },
    "include": [
        "Src/**/*"
    ],
    "references": [
        {
            "path": ".././SubModule/KDInterface"
        }
    ]
}